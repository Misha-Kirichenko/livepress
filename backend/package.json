{
	"name": "livepress",
	"version": "1.0.0",
	"description": "Live notify about articles",
	"main": "src/app.js",
	"scripts": {
		"start": "node src/app.js",
		"migrate:all": "node src/migrations/index.js",
		"seed:all": "node src/seeders/index.js",
		"install_migrate_seed_and_start": "npm i && npm run migrate:all && npm run seed:all && npm run start"
	},
	"keywords": [
		"livepress",
		"sockets"
	],
	"author": "Misha Kirichenko",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-validator": "^7.2.1",
		"he": "^1.2.0",
		"htmlparser2": "^10.0.0",
		"jsonwebtoken": "^9.0.2",
		"module-alias": "^2.2.3",
		"multer": "^1.4.5-lts.1",
		"npm": "^10.8.1",
		"path": "^0.12.7",
		"pg": "^8.12.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.37.3",
		"socket.io": "^4.8.1"
	},
	"_moduleAliases": {
		"@root": ".",
		"@constants": "src/constants",
		"@models": "src/models",
		"@controllers": "src/controllers",
		"@services": "src/services",
		"@config": "src/config",
		"@utils": "src/utils",
		"@middlewares": "src/middlewares",
		"@sockets": "src/sockets"
	},
	"devDependencies": {
		"faker": "^5.5.3"
	}
}
